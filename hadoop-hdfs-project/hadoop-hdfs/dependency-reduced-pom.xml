                  <propertyset>
                    <propertyref />
                  </propertyset>
                </echoproperties>
                <filter />
                <copy />
                <copy />
                <copy>
                  <fileset>
                    <exclude />
                    <exclude />
                  </fileset>
                </copy>
              </target>
            </configuration>
          </execution>
          <execution>
            <id>create-log-dir</id>
